---
interface Props {
    error: unknown
}

const { error } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>{error instanceof Error ? error.message : 'Unknown error'}</div>
</body>
</html>